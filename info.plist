				<key>script</key>
				<string>localgetway=$(route -n get default | grep 'gateway' | awk '{print $2}')
localipv4=$(ifconfig | grep 'inet.*broadcast' -m 1 | awk '{print $2}')
externalipv4=$(curl icanhazip.com)
address=$(curl  -4 -s -m 5 myip.ipip.net | awk '{print $3$4$5$6}' | sed 's/来自于：*//')
localipv6=$(ifconfig | grep 'inet6.*%en' -m 1 | awk '{print $2}' | sed 's/%en0*//')
externalipv6=$(curl -6 -s -m 5 icanhazip.com)
nameserver=$(cat /etc/resolv.conf | grep 'nameserver' | awk '{print $2}' | tr '\n' ';' | rev | cut -c 2- | rev)


subtitletext='Hold down the ⌘ and then press enter to open current url'


if [ -z $localgetway ]
  then
  localgetway='n/a'
fi

if [ -z $localipv4 ]
  then
  localipv4='n/a'
fi

if [ -z $externalipv4 ]
  then
  externalipv4='n/a'
fi

if [ -z $address ]
  then
  address='n/a'
fi

if [ -z $localipv6 ]
  then
  localipv6='n/a'
fi

if [ -z $externalipv6 ] || [ $externalipv6 == $externalipv4 ]
  then
  externalipv6='n/a'
fi

if [ -z $nameserver]
  then
  nameserver='n/a'
fi

cat &lt;&lt; EOB
{"items": [

  {
    "title": "Local_Getway: $localgetway",
    "subtitle": "$subtitletext",
    "arg": "$localgetway"
  },

  {
    "title": "Local_IPv4: $localipv4",
    "subtitle": "$subtitletext",
    "arg": "$localipv4"
  },

  {
    "title": "External_IPv4: $externalipv4",
    "arg": "$externalipv4"
  },

  {	
    "title": "IPv4_From: $address",
    "arg": "$IPv4Address"
  },

  {
    "title": "Local_IPv6: $localipv6",
    "arg": "$localipv6"
  },

  {
    "title": "External_IPv6: $externalipv6",
    "arg": "$externalipv6"
  },

  {
    "title": "DNS: $nameserver",
    "arg": "$nameserver"
  }

]}
EOB
